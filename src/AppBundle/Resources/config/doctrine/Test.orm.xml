<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Test" table="test">
    <id name="idtest" type="integer" column="idtest">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cualidad" type="string" column="cualidad" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valortest" type="string" column="valortest" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="unidades" type="string" column="unidades" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="observaion" type="string" column="observaion" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idprofesor" type="integer" column="idprofesor" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idalumno" type="integer" column="idalumno" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
