<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Profesor" table="profesor">
    <id name="idprofesor" type="integer" column="idprofesor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idpersona" type="integer" column="idpersona" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="usuario" type="string" column="usuario" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contraseña" type="string" column="contraseña" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="68" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="niveleducativo" type="string" column="niveleducativo" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disciplina" type="string" column="disciplina" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
