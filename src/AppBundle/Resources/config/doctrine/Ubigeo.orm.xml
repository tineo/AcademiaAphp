<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ubigeo" table="ubigeo">
    <id name="idubigeo" type="integer" column="idubigeo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pais" type="string" column="pais" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departamento" type="string" column="departamento" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="provincia" type="string" column="provincia" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="distrito" type="string" column="distrito" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
