<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Asistencia" table="asistencia">
    <id name="idasistencia" type="integer" column="idasistencia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dia" type="datetime" column="dia" nullable="true"/>
    <field name="idalumno" type="integer" column="idalumno" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tipoestado" type="integer" column="tipoestado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
