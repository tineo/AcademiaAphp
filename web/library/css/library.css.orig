}

ul.list-styled,
ul.list-styled-alt,
ul.list-filter{
	list-style-type: none;
	padding-left: 1em;
}

ul.list-filter{
	padding-left: 0;
}

ul.list-styled li,
ul.list-styled-alt li,
ul.list-filter li a{
	position: relative;
}

ul.list-filter li a{
	display: block;
	padding-left: 1em;
}

ul.list-styled li::before,
ul.list-styled-alt li::before,
ul.list-filter li a::before{
	position: absolute;
	left: -1rem;
	top: 2px;	
}

ul.list-filter li a::before{
	left: 0;
}

ul.list-filter li a.btn{
	font-size: .75rem;
	padding: .25em .75em;
}

ul.list-filter.list-inline li{
	padding-right: 0;
}

