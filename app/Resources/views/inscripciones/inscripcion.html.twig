<!-- Start Main Content -->
<section class="main-content">
    <div class="container body-content">
        <h3>Inscripcion de beneficiarios</h3>
        <div class="row">

            <section class="col-md-4">
                <label class="input" for="apellidopa">Apellido Paterno</label>
                <input type="text" class="form-control" placeholder="Apellidos" id="apellidopa" name="apellidopa">
            </section>
            <section class="col-md-4">
                <label class="input" for="apellidoma">Apellido Materno</label>
                <input type="text" class="form-control" placeholder="Apellidos" id="apellidoma" name="apellidoma">
            </section>
            <section class="col-md-4">
                <label class="input" for="nombres">Nombres</label>
                <input type="text" class="form-control" placeholder="Nombres" id="nombres" name="nombres">
            </section>

        </div>


        <div class="row">
            <section class="col-md-6">
                <label class="input" for="dni">DNI</label>
                <input type="text" class="form-control" placeholder="DNI" id="dni" name="dni">
            </section>
            <section class="col-md-6">

                <label class="input">Sexo</label>
                <br/>
                <label class="radio inline">
                    <input type="radio" name="gender" value="male" checked>
                    <span> Hombre </span>
                </label>
                <label class="radio inline">
                    <input type="radio" name="gender" value="female">
                    <span> Mujer </span>
                </label>

            </section>

        </div>

        <div class="row">
            <section class="col-md-2">
                <label class="input"> Discapacidad</label>
                <br/>

                <label class="radio inline">
                    <input type="radio" name="discapacidad" value="si" checked>
                    <span> Si </span>
                </label>
                <label class="radio inline">
                    <input type="radio" name="discapacidad" value="no">
                    <span> No </span>
                </label>
            </section>
            <section class="col-md-4">

                <label class="input" for="otros">Â¿Cual?</label>
                <input type="text" class="form-control"  id="otros" name="otros">

            </section>
            <section class="col-md-6">
                <label class="input"> Fecha de nacimiento</label>
                <input type="date"  class="form-control" name="bday">
            </section>

        </div>

        <div class="row">

            <section class="col-md-6">
                <label class="input"> Direccion </label>
                <input type="text"  class="form-control" name="direccion">
            </section>

            <section class="col-md-6">
                <label class="input">Complejo o sede</label>
                <select class="form-control">
                {% for complejo in complejos %}
                    <option>{{ complejo.nombre }}</option>
                {% endfor  %}
                </select>
            </section>
        </div>

        <div class="row">
            <section class="col-md-12">
                <label class="input">Horario</label>
                <select class="form-control">
                    <option>L - V : 9am - 2pm</option>
                    <option>M - V : 7am - 3pm</option>
                    <option>J - S : 10am - 5pm</option>

                </select>
            </section>
        </div>

        <h4>Apoderado</h4>

        <div class="row">
            <section class="col-md-4">
                <label class="input" for="anombres">Nombres</label>
                <input type="text" class="form-control" placeholder="Nombres" id="anombres" name="nombres">
            </section>
            <section class="col-md-4">
                <label class="input" for="aapellidopa">Apellido Paterno</label>
                <input type="text" class="form-control" placeholder="Apellidos" id="aapellidopa" name="apellidopa">
            </section>
            <section class="col-md-4">
                <label class="input" for="aapellidoma">Apellido Materno</label>
                <input type="text" class="form-control" placeholder="Apellidos" id="aapellidoma" name="apellidoma">
            </section>

        </div>

        <div class="row">
            <section class="col-md-4">
                <label class="input" for="aafinidad">Afinidad</label>
                <input type="text" class="form-control" placeholder="Afinidad" id="aafinidad" name="nombres">
            </section>
            <section class="col-md-4">
                <label class="input" for="atelefono">Telefono</label>
                <input type="text" class="form-control" placeholder="Telefono" id="atelefono" name="apellidos">
            </section>

            <section class="col-md-4">
                <label class="input" for="anombres">DNI</label>
                <input type="text" class="form-control" placeholder="DNI" id="adni" name="nombres">
            </section>


        </div>


        <div class="row">
            <section class="col-md-12" style=": center">
                <br />
                <button type="submit" class="btn btn-success">Registrar</button>
            </section>
        </div>





    </div><!-- /.container -->
</section><!-- /.main-content -->








